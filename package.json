{
    "name": "posthog-plugin-geoip",
    "version": "0.1.0",
    "description": "Enrich PostHog events and persons with GeoIP location data",
    "keywords": [
        "posthog",
        "plugin",
        "maxmind",
        "geoip",
        "geolite"
    ],
    "main": "dist/index.js",
    "repository": "github:PostHog/posthog-plugin-geoip",
    "bugs": {
        "url": "https://github.com/PostHog/posthog-plugin-geoip/issues"
    },
    "homepage": "https://github.com/PostHog/posthog-plugin-geoip#readme",
    "author": "PostHog <hey@posthog.com>",
    "scripts": {
        "test": "jest src",
        "build": "yarn clean && yarn compile",
        "clean": "rimraf dist/*",
        "purge": "rimraf dist node_modules",
        "compile": "rollup -c",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prepublishOnly": "yarn test && yarn build"
    },
    "dependencies": {},
    "devDependencies": {
        "@posthog/plugin-scaffold": "0.4.2",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@rollup/plugin-typescript": "^8.2.0",
        "@types/jest": "^26.0.19",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "eslint": "^7.0.0",
        "husky": "~4.3.6",
        "jest": "^26.6.3",
        "lint-staged": "~10.5.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.36.1",
        "rollup-plugin-dts": "^2.0.1",
        "rollup-plugin-node-builtins": "^2.1.2",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && tsc --noEmit"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx,json,yaml,toml,css,scss,html,xml}": "prettier --write"
    }
}
